<?xml version="1.0" encoding="UTF-8" ?>
<root>
	<entry>
		<id>xzycTable</id>
		<title>修正结果确认LIST表</title>
		<type>2</type>
		<excel><![CDATA[/WEB-INF/report/template/异常处理/修正结果确认LIST.xls]]></excel>
		<sheet-index></sheet-index>
		<row-index></row-index>
		<cell-index></cell-index>
		<seq-index></seq-index>
		<modules>
			<module>
				<name>A</name>
				<sql><![CDATA[
				select JBNO_,XMMC_,BGQZ_, BGHZ_,XGR_,XGSJ_ from SINO_JLBG where MKLX_='0' and#1#  GROUP BY XGSJ_,JBNO_, XMMC_,BGQZ_, BGHZ_,XGR_
				]]></sql>
			</module>
		</modules>
		<params>
			<param>
				<name>修正日期</name>
				<column>XGSJ_</column>
				<type>date</type>
				<opt>eq</opt>
				<required>false</required>
			</param>
			<param>
				<name>COIL/PLIE No.</name>
				<column>JBNO_</column>
				<type>string</type>
				<opt>eq</opt>
				<required>false</required>
				<length>11</length>
			</param>
		</params>
	</entry>
	<entry>
		<id>scycTable</id>
		<title>生成结果确认LIST</title>
		<type>2</type>
		<excel><![CDATA[/WEB-INF/report/template/异常处理/生成结果确认LIST.xls]]></excel>
		<sheet-index></sheet-index>
		<row-index></row-index>
		<cell-index></cell-index>
		<seq-index></seq-index>
		<modules>
			<module>
				<name>A</name>
				<sql><![CDATA[
				select JBNO_, NJBNO_,CONVERT(VARCHAR(10), XGSJ_, 111),XMMC_,BGQZ_, BGHZ_ from SINO_JLBG where MKLX_='1' and#1# order by XGSJ_ desc, JBNO_ asc
				]]></sql>
			</module>
		</modules>
		<params>
			<param>
				<name>生成日期</name>
				<column>XGSJ_</column>
				<type>date</type>
				<opt>eq</opt>
				<required>false</required>
			</param>
			<param>
				<name></name>
				<column></column>
				<type></type>
				<opt></opt>
				<required></required>
			</param>
			<param>
				<name></name>
				<column></column>
				<type></type>
				<opt></opt>
				<required></required>
			</param>
			<param>
				<name>引用制品号</name>
				<column>JBNO_</column>
				<type>string</type>
				<opt>eq</opt>
				<required>false</required>
				<length>11</length>
			</param>
			<param>
				<name>新制品号</name>
				<column>NJBNO_</column>
				<type>string</type>
				<opt>eq</opt>
				<required>false</required>
				<length>11</length>
			</param>
		</params>
	</entry>
	<entry>
		<id>shcycTable</id>
		<title>删除结果确认LIST</title>
		<type>2</type>
		<excel><![CDATA[/WEB-INF/report/template/异常处理/删除结果确认LIST.xls]]></excel>
		<sheet-index></sheet-index>
		<row-index></row-index>
		<cell-index></cell-index>
		<seq-index></seq-index>
		<modules>
			<module>
				<name>A</name>
				<sql><![CDATA[
				select ZZSD_+'-'+JBNO_,XGR_,XGSJ_ from SINO_XPXX where MKLX_='2' and#1# ORDER BY XGSJ_ ASC
				]]></sql>
			</module>
		</modules>
		<params>
			<param>
				<name>删除日期</name>
					<column>XGSJ_</column>
				<type>date</type>
				<opt>eq</opt>
				<required>false</required>
			</param>
			<param>
				<name>COIL/PLIE No.</name>
				<column>JBNO_</column>
				<type>string</type>
				<opt>eq</opt>
				<required>false</required>
				<length>11</length>
			</param>
		</params>
	</entry>
	<entry>
		<id>zyycTable</id>
		<title>作业停止日志</title>
		<type>2</type>
		<excel><![CDATA[/WEB-INF/report/template/异常处理/作业停止日志.xls]]></excel>
		<sheet-index>0,1</sheet-index>
		<row-index></row-index>
		<cell-index></cell-index>
		<seq-index></seq-index>
		<modules>
			<module>
				<name>A</name>
				<sql><![CDATA[
				select a.CZR_ ,a.CZSJ_,a.DHNO_,a.NO_,a.ZTBJ_,a.ZYLY_,a.ABBR_,a.XPCC_,a.GGNO_,a.FUZL_,a.CZ_,a.JCR_,a.JCSJ_,case when a.JCLY_ is not null then a.JCLY_ else a.JCCZ_ end from SINO_V_YCCL_ZYRZ a where#1# ORDER BY a.CZSJ_ ASC, a.JBNO_ ASC 
				]]></sql>
			</module>
			<module>
				<name>B</name>
				<sql><![CDATA[
				select a.CZR_ ,CONVERT(VARCHAR(10), a.CZSJ_, 120) AS CZSJ_,a.DHNO_+a.LINE_,a.NO_,a.ZPZL_,a.FACE_,a.ZTBJ_,a.ZYLY_,a.ABBR_,a.XPCC_,a.GGNO_,a.FUZL_,a.CZ_ from SINO_V_XP_ZYTZ a where#1# ORDER BY a.CZSJ_ ASC,a.NO_ ASC
				]]></sql>
			</module>	
		</modules>
		<params>
			<param>
				<name>设定日期</name>
				<column>a.CZSJ_</column>
				<type>date</type>
				<opt>between</opt>
				<required>false</required>
			</param>
			<param>
				<name>制品号</name>
				<column>a.JBNO_</column>
				<type>string</type>
				<opt>eq</opt>
				<required>false</required>
				<length>11</length>
			</param>
		</params>
	</entry>
	<entry>
		<id>xpzytz</id>
		<title>现品作业停止统计</title>
		<type>2</type>
		<excel><![CDATA[/WEB-INF/report/template/异常处理/现品作业停止统计.xls]]></excel>
		<sheet-index>0</sheet-index>
		<row-index></row-index>
		<cell-index></cell-index>
		<seq-index></seq-index>
		<modules>
			<module>
				<name>A</name>
				<sql><![CDATA[
				select a.CZR_ ,CONVERT(VARCHAR(10), a.CZSJ_, 120) AS CZSJ_,a.DHNO_+a.LINE_,a.NO_,a.ZTBJ_,a.ZYLY_,a.ABBR_,a.XPCC_,a.GGNO_,a.FUZL_,a.CZ_ from SINO_V_XP_ZYTZ a where#1# ORDER BY a.CZSJ_ ASC,a.NO_ ASC 
				]]></sql>
			</module>
		</modules>
		<params>
			<param>
				<name>设定日期</name>
				<column>a.CZSJ_</column>
				<type>date</type>
				<opt>between</opt>
				<required>false</required>
			</param>
			<param>
				<name>制品号</name>
				<column>a.JBNO_</column>
				<type>string</type>
				<opt>eq</opt>
				<required>false</required>
				<length>11</length>
			</param>
			<param>
				<name></name>
				<column></column>
				<type></type>
				<opt></opt>
				<required></required>
			</param>
			<param>
				<name>订货No</name>
				<column>a.DHNO_</column>
				<type>string</type>
				<opt>between</opt>
				<required>false</required>
				<length>7</length>
				<jointChar>-</jointChar>
				<jointColumn>a.LINE_</jointColumn>
				<jointLength>2</jointLength>
			</param>
			<param>
				<name>是否余材</name>
				<column>a.SFYC_</column>
				<type>string</type>
				<opt>eq</opt>
				<select>
				<![CDATA[
				<option value="">全部</option>
				<option value="Y">是</option>
				<option value="N">否</option>
				]]>
				</select>
				<required>false</required>
				<length>4</length>
			</param>
		</params>
	</entry>
	<entry>
		<id>ylycTable</id>
